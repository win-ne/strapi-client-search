{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "develop": {
            "cache": false
        },
        "dev": {
            "cache": false,
            "dependsOn": [
                "develop"
            ]
        },
        "dma-backend#build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**"
            ]
        },
        "generate-search-assets": {
            "cache": false
        }
    }
}