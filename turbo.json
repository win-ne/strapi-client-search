{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "develop": {
            "cache": false
        },
        "dev": {
            "cache": false,
            "dependsOn": [
                "develop"
            ]
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**"
            ]
        }
    }
}